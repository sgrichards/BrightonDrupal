{% if class_name is defined %}

{{ module_name }}.{{form_id}}:
  path: '/admin/config/{{ module_name }}/{{ class_name_short }}'
  defaults:
    _form: '\Drupal\{{ module_name }}\Form\{{ class_name }}'
    _title: '{{ class_name }}'
  requirements:
    _permission: 'access administration pages'
  options:
    _admin_route: TRUE
{% endif %}

